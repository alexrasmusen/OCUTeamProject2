<?import javafx.scene.text.*?>

<AnchorPane prefHeight="350.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.okcu.project2.ProfessorLoginController">
    <children>
        <Label fx:id="lblName" layoutX="74.0" layoutY="126.0" prefHeight="17.0" prefWidth="58.0" text="Name" />
        <TextField fx:id="txtfieldName" layoutX="74.0" layoutY="144.0" />
        <Label fx:id="lblEmail" layoutX="74.0" layoutY="169.0" text="Email" />
        <TextField fx:id="txtfieldEmail" layoutX="74.0" layoutY="188.0" />
        <Label fx:id="lblPassword" layoutX="74.0" layoutY="217.0" text="Password" />
      <PasswordField fx:id="txtfieldPassword" layoutX="74.0" layoutY="234.0" />
        <Button fx:id="buttonSignUp" layoutX="120.0" layoutY="274.0" mnemonicParsing="false" onAction="#onLoginPress" text="Login" />
