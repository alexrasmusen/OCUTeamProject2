<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="450.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.okcu.project2.ProfessorViewStudentsController">
   <children>
      <TableView fx:id="tableView" layoutX="-1.0" layoutY="30.0" prefHeight="238.0" prefWidth="500.0">
        <columns>
          <TableColumn fx:id="IDColumn" prefWidth="58.0" text="ID" />
          <TableColumn fx:id="nameColumn" minWidth="0.0" prefWidth="139.0" text="Name" />
            <TableColumn fx:id="emailColumn" prefWidth="245.0" text="Email" />
            <TableColumn fx:id="gradeColumn" prefWidth="57.0" text="Grade" />
        </columns>
      </TableView>
      <Label fx:id="lblWelcomeMessage" layoutX="168.0" layoutY="6.0" text="Here are your students in _____" />
      <HBox layoutX="24.0" layoutY="399.0" prefHeight="27.0" prefWidth="450.0" spacing="30.0">
         <children>
            <Button fx:id="btnAdd" mnemonicParsing="false" onAction="#onAddButtonClick" text="Add" />
            <Button fx:id="btnUpdate" mnemonicParsing="false" onAction="#onUpdateButtonClick" text="Update" />
            <Button fx:id="btnDelete" mnemonicParsing="false" onAction="#onDeleteButtonClick" text="Delete" />
            <Button fx:id="btnClear" mnemonicParsing="false" onAction="#onClearButtonClick" text="Clear" />
            <Button mnemonicParsing="false" onAction="#onReturnButtonClick" text="Return" />
            <Button mnemonicParsing="false" onAction="#onSignOutButtonClick" text="Signout" />
         </children>
      </HBox>
      <VBox layoutX="44.0" layoutY="298.0" prefHeight="86.0" prefWidth="180.0">
         <children>
            <Label text="ID" />
            <Label fx:id="lblID" prefHeight="24.0" prefWidth="180.0" />
            <Label text="Name" />
            <TextField fx:id="txtName" />
         </children>
      </VBox>
      <VBox layoutX="257.0" layoutY="298.0" prefHeight="86.0" prefWidth="180.0">
         <children>
            <Label text="Email" />
            <TextField fx:id="txtEmail" />
            <Label text="Grade" />
            <TextField fx:id="txtGrade" />
         </children>
      </VBox>
      <Label fx:id="lblError" layoutX="224.0" layoutY="275.0" text="Label" />
   </children>
</AnchorPane>
