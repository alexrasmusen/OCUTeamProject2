<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox prefHeight="316.0" prefWidth="294.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.okcu.project2.WelcomeController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="200.0" spacing="40.0">
         <children>
            <Label text="Student ">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <Button fx:id="StudentLButton" mnemonicParsing="false" onAction="#onStuLoginButtonClick" text="Login" />
            <Button fx:id="StudentSUButton" mnemonicParsing="false" onAction="#onStuSignUpButtonClick" text="SignUp" />
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="200.0" spacing="40.0">
         <children>
            <Label text="Professor">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#onProfLoginButtonClick" text="Login" />
            <Button mnemonicParsing="false" onAction="#onProfSignUpButtonClick" text="SignUp" />
         </children>
      </VBox>
   </children>
</HBox>
