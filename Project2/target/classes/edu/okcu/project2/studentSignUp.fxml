<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="350.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.okcu.project2.StudentSignUpController">
   <children>
      <VBox alignment="CENTER" layoutY="18.0" prefHeight="331.0" prefWidth="300.0" spacing="9.0">
         <children>
            <Label fx:id="lblSignUp" prefHeight="58.0" prefWidth="300.0" text="     Student Sign Up">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="lblName" prefHeight="17.0" prefWidth="149.0" text="Name" />
            <TextField fx:id="txtfieldEmail" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" />
            <Label fx:id="lblEmail" prefHeight="17.0" prefWidth="150.0" text="Email" />
            <TextField fx:id="txtfieldName" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" />
            <Label fx:id="lblPassword" prefHeight="17.0" prefWidth="150.0" text="Password" />
            <PasswordField fx:id="txtfieldPassword" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" />
            <Button fx:id="buttonSignUp" mnemonicParsing="false" onAction="#onSignUpClick" text="Sign Up" />
            <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#onCancelButtonClick" text="Cancel" />
         </children>
      </VBox>
   </children>
</AnchorPane>
