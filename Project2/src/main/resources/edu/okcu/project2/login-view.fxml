<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.okcu.project2.LoginController">
   <children>
      <Label fx:id="lblLogin" text="Login">
         <font>
            <Font size="30.0" />
         </font></Label>
      <Label fx:id="lblEmail" text="Email">
         <graphic>
            <TextField fx:id="txtfieldEmail" />
         </graphic>
      </Label>
      <Label fx:id="lblPassword" text="Password">
         <graphic>
            <TextField fx:id="txtfieldPassword" />
         </graphic>
      </Label>
      <Button fx:id="buttonSecondLogin" mnemonicParsing="false" onAction="#onSecondLoginButtonClick" text="Login" />
      <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#onSecondCancelButtonClick" text="Cancel" />
   </children>
</VBox>
