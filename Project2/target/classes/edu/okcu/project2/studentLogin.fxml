<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="350.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.okcu.project2.StudentLoginController">
   <children>
      <VBox alignment="CENTER" layoutY="25.0" prefHeight="325.0" prefWidth="300.0" spacing="10.0">
         <children>
            <Label fx:id="lblLogin" text="Student Login">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="lblName" prefHeight="16.0" prefWidth="149.0" text="Name" />
            <TextField fx:id="txtfieldName" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="150.0" />
            <Label fx:id="lblEmail" prefHeight="17.0" prefWidth="151.0" text="Email" />
            <TextField fx:id="txtfieldEmail" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" />
            <Label fx:id="lblPassword" prefHeight="17.0" prefWidth="150.0" text="Password" />
            <PasswordField fx:id="txtfieldPassword" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" />
            <Button fx:id="buttonSignUp" mnemonicParsing="false" onAction="#onLoginPress" text="Login" />
            <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#onSecondCancelButtonClick" text="Cancel" />
         </children></VBox>
   </children>
</AnchorPane>
