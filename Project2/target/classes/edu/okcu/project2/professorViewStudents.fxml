<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="450.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.okcu.project2.ProfessorViewStudentsController">
   <children>
      <TableView fx:id="tableView" layoutX="15.0" layoutY="30.0" prefHeight="238.0" prefWidth="521.0">
        <columns>
          <TableColumn fx:id="IDColumn" prefWidth="63.0" text="ID" />
          <TableColumn fx:id="nameColumn" minWidth="0.0" prefWidth="177.0" text="Name" />
            <TableColumn fx:id="emailColumn" prefWidth="218.0" text="Email" />
            <TableColumn fx:id="gradeColumn" prefWidth="62.0" text="Grade" />
        </columns>
      </TableView>
      <Label fx:id="lblWelcomeMessage" layoutX="194.0" layoutY="6.0" text="Here are your students in _____" />
      <HBox alignment="CENTER" layoutX="13.0" layoutY="399.0" prefHeight="27.0" prefWidth="528.0" spacing="30.0">
         <children>
            <Button fx:id="btnAdd" mnemonicParsing="false" onAction="#onAddButtonClick" text="Add" />
            <Button fx:id="btnUpdate" mnemonicParsing="false" onAction="#onUpdateButtonClick" text="Update" />
            <Button fx:id="btnDelete" mnemonicParsing="false" onAction="#onDeleteButtonClick" text="Delete" />
            <Button fx:id="btnClear" mnemonicParsing="false" onAction="#onClearButtonClick" text="Deselect" />
            <Button fx:id="btnReturn" mnemonicParsing="false" onAction="#onReturnButtonClick" text="Return" />
            <Button fx:id="btnSignout" mnemonicParsing="false" onAction="#onSignoutButtonClick" text="Signout" />
         </children>
      </HBox>
      <VBox layoutX="66.0" layoutY="298.0" prefHeight="86.0" prefWidth="180.0">
         <children>
            <Label text="ID" />
            <Label fx:id="lblID" prefHeight="24.0" prefWidth="180.0" />
            <Label text="Name" />
            <TextField fx:id="txtName" />
         </children>
      </VBox>
      <VBox layoutX="292.0" layoutY="298.0" prefHeight="86.0" prefWidth="180.0">
         <children>
            <Label text="Email" />
            <TextField fx:id="txtEmail" />
            <Label text="Grade" />
            <TextField fx:id="txtGrade" />
         </children>
      </VBox>
      <Label fx:id="lblError" layoutX="261.0" layoutY="274.0" text="Label" />
   </children>
</AnchorPane>
